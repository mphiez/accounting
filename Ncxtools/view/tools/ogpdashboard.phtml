	function load_quote(){
		
		document.getElementById('quote_foot').style.display = '';
		
		var data = {
			"from" : $('#quote_from').val(),
			"to" : $('#quote_to').val(),
			"status_quote" : $('#status_quote').val(),
			"umur_quote" : $('#umur_quote').val(),
