		}
		
		document.getElementById('qoute').innerHTML = "";
		
		$.ajax({
				url: '<?php echo $this->basePath()?>/ncxtools/data/load-quote',
				type: "POST",
				data: {data},
				success: function(datax) {
					document.getElementById('quote_foot').style.display = 'none';
					if(datax.code == 0){
						var row1 = 0;
						var row2 = 0;
						var row3 = 0;
						var row4 = 0;
