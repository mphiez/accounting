							$('#qoute').append('<tr>'+
							'<td style="background-color:lightgray;" onclick="return view(4,1,&#39;'+item.STATUS_QUOTE+'&#39;,&#39;&#39;)"><b>'+item.STATUS_QUOTE+'</td>'+
							'<td style="text-align:center;background-color:#ef9c28;color:white;" onclick="return view(4,2,&#39;'+item.PROCESS+'&#39;,&#39;'+item.STATUS_QUOTE+'&#39;)"><b>'+item.DBS+'</td>'+
